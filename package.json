{
  "name": "hypercore-stats-ui",
  "version": "1.1.0",
  "description": "HTML based stats ui for hypercore/hyperdrive",
  "main": "index.js",
  "dependencies": {
    "browserify": "^13.0.1",
    "corsify": "^2.1.0",
    "event-source-stream": "^1.5.0",
    "pixel-grid": "^1.0.0",
    "pretty-bytes": "^3.0.1",
    "speedometer": "^1.0.0"
  },
  "scripts": {
    "build": "browserify example/app.js > example/bundle.js && npm run build-css",
    "build-css": "node-sass --importer node_modules/node-sass-magic-importer example/main.scss example/main.css",
    "watch-css": "nodemon -e example/main.scss -x \"npm run build-css\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mafintosh/hypercore-stats-ui.git"
  },
  "author": "Mathias Buus (@mafintosh)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mafintosh/hypercore-stats-ui/issues"
  },
  "homepage": "https://github.com/mafintosh/hypercore-stats-ui",
  "devDependencies": {
    "dat-design": "^1.2.11",
    "hypercore-stats-server": "^1.0.0",
    "hyperdrive-archive-swarm": "^3.1.0",
    "memdb": "^1.3.1",
    "minimist": "^1.2.0",
    "node-sass-magic-importer": "^0.1.4",
    "nodemon": "^1.10.0"
  }
}
